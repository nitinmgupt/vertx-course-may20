<?xml version="1.0" encoding="UTF-8"?>
<testsuite name="{{fullName}}" time="{{elapsedTimeAsString}}" tests="1" errors="{{errorAsInt}}"
           skipped="0"
           failures="{{failureAsInt}}">
    <properties>
    </properties>
    <testcase name="{{fullName}}" classname="{{fullName}}" time="{{elapsedTimeAsString}}">
        {{#if failed}}
        <failure type="java.lang.AssertionError:">
            {{exception}}
        </failure>
        {{/if}}
    </testcase>
</testsuite>